	<!-- INSERT EMP TREE DISTRIBUTION HERE -->
	<empiricalTreeDistributionModel id="treeModel" fileName="UP_BS10_strict.trees">
		<taxa idref="taxa"/>
	</empiricalTreeDistributionModel>	
	<statistic id="treeModel.currentTree" name="Current Tree">
		<empiricalTreeDistributionModel idref="treeModel"/>
	</statistic>
	
	<generalSubstitutionModel id="binaryGeo.model" randomizeIndicator="false">
		<generalDataType idref="binaryGeo.dataType"/>
		<frequencies>
			<frequencyModel id="binaryGeo.frequencyModel" normalize="true">
				<generalDataType idref="binaryGeo.dataType"/>
				<frequencies>
					<parameter id="binaryGeo.frequencies" dimension="2"/>
				</frequencies>
			</frequencyModel>
		</frequencies>
		<rates>
			<parameter id="binaryGeo.rates" dimension="2" value="1.0"/>
		</rates>
		<rateIndicator>
			<parameter id="binaryGeo.indicators" dimension="2" value="1.0"/>
		</rateIndicator>
	</generalSubstitutionModel>
	
	<sumStatistic id="binaryGeo.nonZeroRates" elementwise="true">
		<parameter idref="binaryGeo.indicators"/>
	</sumStatistic>
	<productStatistic id="binaryGeo.actualRates" elementwise="false">
		<parameter idref="binaryGeo.indicators"/>
		<parameter idref="binaryGeo.rates"/>
	</productStatistic>
	<siteModel id="binaryGeo.siteModel">
		<substitutionModel>
			<generalSubstitutionModel idref="binaryGeo.model"/>
		</substitutionModel>
	</siteModel>

	<generalSubstitutionModel id="species.model" randomizeIndicator="false">
		<generalDataType idref="species.dataType"/>
		<frequencies>
			<frequencyModel id="species.frequencyModel" normalize="true">
				<generalDataType idref="species.dataType"/>
				<frequencies>
					<parameter id="species.frequencies" dimension="4"/>
				</frequencies>
			</frequencyModel>
		</frequencies>
		<rates>
			<parameter id="species.rates" dimension="12" value="1.0"/>
		</rates>
		<rateIndicator>
			<parameter id="species.indicators" dimension="12" value="1.0"/>
		</rateIndicator>
	</generalSubstitutionModel>
	<sumStatistic id="species.nonZeroRates" elementwise="true">
		<parameter idref="species.indicators"/>
	</sumStatistic>
	<productStatistic id="species.actualRates" elementwise="false">
		<parameter idref="species.indicators"/>
		<parameter idref="species.rates"/>
	</productStatistic>
	<siteModel id="species.siteModel">
		<substitutionModel>
			<generalSubstitutionModel idref="species.model"/>
		</substitutionModel>
	</siteModel>

	<ancestralTreeLikelihood id="binaryGeo.treeLikelihood" stateTagName="binaryGeo.states">
		<attributePatterns idref="binaryGeo.pattern"/>
		<treeModel idref="treeModel"/>
		<siteModel idref="binaryGeo.siteModel"/>
		<generalSubstitutionModel idref="binaryGeo.model"/>
		<frequencyModel id="binaryGeo.root.frequencyModel" normalize="true">
			<generalDataType idref="binaryGeo.dataType"/>
			<frequencies>
				<parameter id="binaryGeo.root.frequencies" dimension="2"/>
			</frequencies>
		</frequencyModel>		
	</ancestralTreeLikelihood>
	<ancestralTreeLikelihood id="species.treeLikelihood" stateTagName="species.states">
		<attributePatterns idref="species.pattern"/>
		<treeModel idref="treeModel"/>
		<siteModel idref="species.siteModel"/>
		<generalSubstitutionModel idref="species.model"/>
		<frequencyModel id="species.root.frequencyModel" normalize="true">
			<generalDataType idref="species.dataType"/>
			<frequencies>
				<parameter id="species.root.frequencies" dimension="4"/>
			</frequencies>
		</frequencyModel>
	</ancestralTreeLikelihood>


	<operators id="operators" optimizationSchedule="log">
		<empiricalTreeDistributionOperator weight="1">
			<empiricalTreeDistributionModel idref="treeModel"/>
		</empiricalTreeDistributionOperator>
		<scaleOperator scaleFactor="0.75" weight="15" scaleAllIndependently="true">
			<parameter idref="binaryGeo.rates"/>
		</scaleOperator>
		<!--
		<bitFlipOperator weight="7">
			<parameter idref="binaryGeo.indicators"/>
		</bitFlipOperator>
		-->
		<scaleOperator scaleFactor="0.75" weight="15" scaleAllIndependently="true">
			<parameter idref="species.rates"/>
		</scaleOperator>
		<!--
		<bitFlipOperator weight="7">
			<parameter idref="species.indicators"/>
		</bitFlipOperator>
		-->
		<deltaExchange delta="0.75" weight="1">
			<parameter idref="binaryGeo.root.frequencies"/>
		</deltaExchange>
		<deltaExchange delta="0.75" weight="1">
			<parameter idref="species.root.frequencies"/>
		</deltaExchange>		
	</operators>
	<mcmc id="mcmc" chainLength="20000000" autoOptimize="true" operatorAnalysis="UP_HostGeoTraits.ops">
		<posterior id="posterior">
			<prior id="prior">
				<poissonPrior mean="1.0" offset="1.0">
					<statistic idref="binaryGeo.nonZeroRates"/>
				</poissonPrior>
				<uniformPrior lower="0.0" upper="1.0">
					<parameter idref="binaryGeo.frequencies"/>
				</uniformPrior>
				<cachedPrior>
					<gammaPrior shape="1.0" scale="1.0" offset="0.0">
						<parameter idref="binaryGeo.rates"/>
					</gammaPrior>
					<parameter idref="binaryGeo.rates"/>
				</cachedPrior>
				<poissonPrior mean="5.0" offset="0.0">
					<statistic idref="species.nonZeroRates"/>
				</poissonPrior>
				<uniformPrior lower="0.0" upper="1.0">
					<parameter idref="species.frequencies"/>
				</uniformPrior>
				<cachedPrior>
					<gammaPrior shape="1.0" scale="1.0" offset="0.0">
						<parameter idref="species.rates"/>
					</gammaPrior>
					<parameter idref="species.rates"/>
				</cachedPrior>
				<generalSubstitutionModel idref="binaryGeo.model"/>
				<generalSubstitutionModel idref="species.model"/>
				<uniformPrior lower="0.0" upper="1.0">
					<parameter idref="binaryGeo.root.frequencies"/>
				</uniformPrior>
				<uniformPrior lower="0.0" upper="1.0">
					<parameter idref="species.root.frequencies"/>
				</uniformPrior>				
			</prior>
			<likelihood id="likelihood">
				<ancestralTreeLikelihood idref="binaryGeo.treeLikelihood"/>
				<ancestralTreeLikelihood idref="species.treeLikelihood"/>
			</likelihood>
		</posterior>
		<operators idref="operators"/>

		<log id="screenLog" logEvery="5000">
			<column label="Posterior" dp="4" width="12">
				<posterior idref="posterior"/>
			</column>
			<column label="Prior" dp="4" width="12">
				<prior idref="prior"/>
			</column>
			<column label="Likelihood" dp="4" width="12">
				<likelihood idref="likelihood"/>
			</column>
			<column label="binaryGeo.nonZeroRates" sf="6" width="12">
				<sumStatistic idref="binaryGeo.nonZeroRates"/>
			</column>
			<column label="species.nonZeroRates" sf="6" width="12">
				<sumStatistic idref="species.nonZeroRates"/>
			</column>
		</log>

		<log id="fileLog" logEvery="5000" fileName="UP_HostGeoTraits.log" overwrite="false">
			<posterior idref="posterior"/>
			<prior idref="prior"/>
			<likelihood idref="likelihood"/>
			<parameter idref="binaryGeo.rates"/>
			<parameter idref="binaryGeo.indicators"/>
			<sumStatistic idref="binaryGeo.nonZeroRates"/>
			<productStatistic idref="binaryGeo.actualRates"/>
			<parameter idref="species.rates"/>
			<parameter idref="species.indicators"/>
			<sumStatistic idref="species.nonZeroRates"/>
			<productStatistic idref="species.actualRates"/>
		</log>

		<log id="AllU_HostGeoTraits.binaryGeorateMatrixLog" logEvery="5000" fileName="UP_binaryGeorates.log">
			<parameter idref="binaryGeo.rates"/>
			<parameter idref="binaryGeo.indicators"/>
			<sumStatistic idref="binaryGeo.nonZeroRates"/>
			<productStatistic idref="binaryGeo.actualRates"/>			
		</log>
		<log id="AllU_HostGeoTraits.speciesrateMatrixLog" logEvery="5000" fileName="UP_speciesrates.log">
			<parameter idref="species.rates"/>
			<parameter idref="species.indicators"/>
			<sumStatistic idref="species.nonZeroRates"/>
			<productStatistic idref="species.actualRates"/>
		</log>
		
		<logTree id="treeFileLog" logEvery="5000" nexusFormat="true" fileName="UP_HostGeoTraits.trees" sortTranslationTable="true">
			<treeModel idref="treeModel"/>
			<posterior idref="posterior"/>
			<trait name="binaryGeo.states" tag="binaryGeo">
				<ancestralTreeLikelihood idref="binaryGeo.treeLikelihood"/>
			</trait>
			<trait name="species.states" tag="species">
				<ancestralTreeLikelihood idref="species.treeLikelihood"/>
			</trait>
		</logTree>
	</mcmc>
	<report>
		<property name="timer">
			<mcmc idref="mcmc"/>
		</property>
	</report>
</beast>
